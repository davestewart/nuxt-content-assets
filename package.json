{
  "name": "nuxt-content-assets",
  "version": "1.3.4",
  "description": "Enable locally-located assets in Nuxt Content",
  "repository": "davestewart/nuxt-content-assets",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev demo",
    "build": "nuxt-module-build",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "demo:prepare": "nuxt-module-build --stub && nuxi prepare demo",
    "demo:generate": "nuxt generate demo",
    "demo:serve": "npx serve demo/dist",
    "demo:build": "nuxi build demo",
    "release": "npm run lint && npm run test && npm run build && npm publish && git push --follow-tags",
    "release:dry": "npm run lint && npm run test && npm run build && npm publish --dry-run"
  },
  "dependencies": {
    "@nuxt/kit": "^3.10.3",
    "crosspath": "^2.0.0",
    "debounce": "^1.2.1",
    "image-size": "^1.0.2",
    "listhen": "^1.7.2",
    "ohash": "^1.0.0",
    "unist-util-visit": "^4.1.2",
    "unstorage": "^1.4.1",
    "ws": "^8.13.0"
  },
  "peerDependencies": {
    "@nuxt/content": "^2.0.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "3.10.3",
    "@nuxt/test-utils": "3.11.0",
    "@types/debounce": "^1.2.1",
    "@types/ws": "^8.5.10",
    "eslint": "^8.57.0",
    "nuxt": "^3.7.0",
    "vitest": "^1.4.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
